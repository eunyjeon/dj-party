{
  "name": "dj_party",
  "version": "3.0.0",
  "description": "How to use create-react-app with a custom Node API on Heroku",
  "engines": {
    "node": "12.16.3",
    "npm": "6.14.8"
  },
  "scripts": {
    "start": "node server/index.js",
    "seed": "node seed/seedScript.js",
    "build": "cd react-ui/ && npm install && npm run build",
    "restart": "cd react-ui/ && npm run build && cd ../ && npm run start"
  },
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "apollo-datasource-rest": "^0.9.3",
    "apollo-server": "^2.6.4",
    "apollo-server-express": "^2.6.4",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-session-sequelize": "^7.0.1",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "graphql": "^15.3.0",
    "lodash": "^4.17.20",
    "merge-graphql-schemas": "^1.7.8",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.4",
    "passport": "^0.4.1",
    "passport-spotify": "^1.1.0",
    "pg": "^8.3.2",
    "pkginfo": "^0.4.1",
    "react": "^16.13.1",
    "sequelize": "^6.3.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mars/heroku-cra-node.git"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "license": "MIT",
  "devDependencies": {
    "fsevents": "^2.1.3"
  }
}
